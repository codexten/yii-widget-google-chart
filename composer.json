{
  "name": "codexten/yii-widget-google-chart",
  "type": "yii2-extension",
  "description": " google-chart widget for yii 2.0 or higher",
  "require": {
    "codexten/yii-asset-vue-google-chart": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\widgets\\GoogleChart\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "sconfig-plugin": {
      "bootstrap3": "config/google-chart.php"
    }
  }
}
